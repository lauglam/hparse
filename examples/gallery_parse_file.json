        {
          "kind": "String",
          "name": "width",
          "description": "缩略图的宽",
          "actions": [
            {
              "error": {
                "kind": "AnyActionAllActionFail",
                "message": "action列表全部执行失败"
              },
              "actions": [
                {
                  "regex": "<img[^>]*style=\"height:(\\d+)px;width:(\\d+)px[^\"]*\"[^>]*src=\"([^\"]+)\"",
                  "group": 2
                },
                {
                  "regex": "width:(\\d+)px; height:(\\d+)px.+?url\\((.+?)\\)",
                  "group": 1
                }
              ]
            }
          ]
        },
        {
          "kind": "String",
          "name": "height",
          "description": "缩略图的高",
          "actions": [
            {
              "error": {
                "kind": "AnyActionAllActionFail",
