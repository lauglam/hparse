            {
              "regex": "https?://(?:exhentai.org|e-hentai.org|lofi.e-hentai.org)/(?:g|mpv)/(\\d+)/([0-9a-f]{10})",
              "group": 2,
              "error": {
                "kind": "RegexNotMatch",
